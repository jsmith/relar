{
  "name": "app",
  "version": "0.5.1",
  "scripts": {
    "dev-mobile": "cp capacitor.dev.json capacitor.config.json && snowpack dev --config snowpack.mobile.json",
    "build-mobile": "cp capacitor.prod.json capacitor.config.json && snowpack build",
    "dev-web": "snowpack dev --config snowpack.web.json",
    "build-web": "snowpack --config snowpack.web.json build",
    "build-staging": "NODE_ENV=staging --config snowpack.web.json snowpack build",
    "test": "./test.sh",
    "check:formatting": "../node_modules/.bin/prettier --list-different \"**/*.{ts,tsx,js}\"",
    "check:lint": "../node_modules/.bin/eslint \"**/*.{ts,tsx}\"",
    "check:types": "../node_modules/.bin/tsc --noEmit",
    "tailwind:dev": "../node_modules/.bin/tailwind build src/shared/web/tailwind.directives.css -o src/shared/web/tailwind.css",
    "tailwind:prod": "NODE_ENV=production ../node_modules/.bin/tailwind build src/shared/web/tailwind.directives.css -o src/shared/web/tailwind.css"
  },
  "dependencies": {
    "@capacitor-community/native-audio": "file:../native-audio",
    "@graywolfai/react-tiniest-router": "^0.3.1",
    "@graywolfai/rest-ts-axios": "^0.2.4",
    "@jsmith21/svg-loaders-react": "^2.2.2",
    "@reach/skip-nav": "^0.11.2",
    "@sentry/browser": "^5.24.2",
    "axios": "^0.19.2",
    "capacitor-blob-writer": "^0.1.10",
    "classnames": "^2.2.6",
    "downshift": "^6.0.6",
    "fast-average-color": "^6.0.3",
    "firebase": "^7.21.1",
    "focus-trap-react": "^8.1.0",
    "framer-motion": "^2.7.6",
    "neverthrow": "^2.7.1",
    "react": "^16.13.1",
    "react-accessible-dropdown-menu-hook": "^2.1.1",
    "react-aria-modal": "^4.0.0",
    "react-dom": "^16.13.1",
    "react-hotkeys-hook": "^2.3.1",
    "react-icons": "^3.11.0",
    "react-infinite-scroll-component": "^5.1.0",
    "react-loading-skeleton": "^2.1.1",
    "react-modal-hook": "^3.0.0",
    "react-query": "^2.23.0",
    "react-query-devtools": "^2.5.1",
    "react-simple-snackbar": "^1.1.10",
    "react-tabs": "^3.1.1",
    "react-useportal": "^1.0.13",
    "runtypes": "^5.0.1",
    "snowpack": "^2.12.1",
    "tinycolor2": "^1.4.2",
    "use-resize-observer": "^6.1.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@capacitor/android": "^2.4.2",
    "@capacitor/cli": "^2.4.2",
    "@capacitor/core": "^2.4.2",
    "@capacitor/ios": "^2.4.2",
    "@snowpack/app-scripts-react": "^1.11.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@tailwindcss/typography": "^0.2.0",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.49",
    "@types/react-aria-modal": "^4.0.2",
    "@types/react-dom": "^16.9.8",
    "@types/react-slider": "^1.0.0",
    "@types/react-tabs": "^2.3.2",
    "@types/tinycolor2": "^1.4.2",
    "@types/uuid": "^8.3.0",
    "dotenv": "^8.2.0",
    "patch-package": "^6.2.2",
    "react-test-renderer": "^16.13.1",
    "tailwindcss": "^1.8.10"
  }
