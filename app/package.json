{
  "name": "app",
  "version": "0.18.1",
  "license": "MPL-2.0",
  "repository": {
    "url": "https://github.com/jsmith/relar"
  },
  "scripts": {
    "clean": "rm -rf build",
    "cp-cap-dev": "cp capacitor.dev.json capacitor.config.json",
    "cp-cap-prod": "cp capacitor.prod.json capacitor.config.json",
    "build:sw": "workbox injectManifest",
    "dev:mobile": "npm run cp-cap-dev && cap copy && SNOWPACK_PUBLIC_MOBILE=1 snowpack dev",
    "build:mobile-production": "npm run clean && npm run cp-cap-prod && SNOWPACK_PUBLIC_MOBILE=1 snowpack build && npm run build:sw",
    "build:mobile-staging": "npm run clean && npm run cp-cap-prod && SNOWPACK_PUBLIC_MOBILE=1 NODE_ENV=staging snowpack build && npm run build:sw",
    "dev:web": "snowpack dev",
    "build:web-production": "npm run clean && snowpack build && npm run build:sw",
    "build:web-staging": "npm run clean && NODE_ENV=staging snowpack build && npm run build:sw",
    "test": "./test.sh",
    "check:formatting": "../node_modules/.bin/prettier --list-different \"**/*.{ts,tsx,js}\"",
    "check:lint": "../node_modules/.bin/eslint \"**/*.{ts,tsx}\"",
    "check:types": "../node_modules/.bin/tsc --noEmit"
  },
  "dependencies": {
    "@capacitor-community/native-audio": "file:../native-audio",
    "@graywolfai/rest-ts-axios": "^0.2.6",
    "@headlessui/react": "^0.2.0",
    "@reach/skip-nav": "^0.11.2",
    "@sentry/browser": "^5.30.0",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "emotion": "^10.0.27",
    "fast-average-color": "^6.3.0",
    "firebase": "^7.23.0",
    "focus-trap-react": "^8.3.2",
    "framer-motion": "^2.9.5",
    "fuse.js": "^6.4.6",
    "idb": "^5.0.8",
    "little-react-router": "^0.1.0",
    "mitt": "^2.1.0",
    "neverthrow": "^2.7.1",
    "react": "^16.14.0",
    "react-accessible-dropdown-menu-hook": "^2.2.0",
    "react-aria-modal": "^4.0.0",
    "react-dom": "^16.14.0",
    "react-hotkeys-hook": "^2.4.1",
    "react-icons": "^3.11.0",
    "react-infinite-scroll-component": "^5.1.0",
    "react-loading-skeleton": "^2.1.1",
    "react-modal-hook": "^3.0.0",
    "react-simple-snackbar": "^1.1.10",
    "react-tabs": "^3.1.2",
    "react-tooltip": "^4.2.12",
    "react-useportal": "^1.0.13",
    "react-virtualized-auto-sizer": "^1.0.3",
    "react-window": "^1.8.6",
    "resize-observer-polyfill": "^1.5.1",
    "runtypes": "^5.0.1",
    "throttle-debounce": "^2.3.0",
    "tinycolor2": "^1.4.2",
    "use-resize-observer": "^6.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@capacitor/android": "^2.4.6",
    "@capacitor/cli": "^2.4.6",
    "@capacitor/core": "^2.4.6",
    "@capacitor/ios": "^2.4.6",
    "@sentry/webpack-plugin": "^1.14.0",
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-postcss": "^1.1.0",
    "@snowpack/plugin-react-refresh": "^2.4.0",
    "@snowpack/plugin-typescript": "^1.2.0",
    "@snowpack/plugin-webpack": "^2.3.0",
    "@testing-library/react-hooks": "^3.7.0",
    "@types/classnames": "^2.2.11",
    "@types/react": "^16.14.2",
    "@types/react-aria-modal": "^4.0.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-slider": "^1.1.0",
    "@types/react-tabs": "^2.3.2",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "@types/throttle-debounce": "^2.1.0",
    "@types/tinycolor2": "^1.4.2",
    "@types/uuid": "^8.3.0",
    "@types/wicg-mediasession": "^1.1.0",
    "autoprefixer": "^10.2.1",
    "dotenv": "^8.2.0",
    "execa": "^4.1.0",
    "fake-indexeddb": "^3.1.2",
    "gm": "^1.23.1",
    "mobisplash": "^0.2.2",
    "pify": "^5.0.0",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "react-test-renderer": "^16.14.0",
    "snowpack": "^3.0.10",
