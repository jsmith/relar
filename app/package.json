{
  "name": "app",
  "version": "0.12.1",
  "scripts": {
    "clean": "rm -rf build",
    "cp-mobile": "cp index.mobile.html public/index.html",
    "cp-web": "cp index.web.html public/index.html",
    "cp-cap-dev": "cp capacitor.dev.json capacitor.config.json",
    "cp-cap-prod": "cp capacitor.prod.json capacitor.config.json",
    "dev:mobile": "npm run cp-mobile && npm run cp-cap-dev && SNOWPACK_PUBLIC_MOBILE=1 snowpack dev --config snowpack.mobile.json",
    "build:mobile-production": "npm run tailwind:prod && npm run clean && npm run cp-mobile && npm run cp-cap-prod && SNOWPACK_PUBLIC_MOBILE=1 snowpack build --config snowpack.mobile.json",
    "build:mobile-staging": "npm run tailwind:prod && npm run clean && npm run cp-mobile && npm run cp-cap-prod && SNOWPACK_PUBLIC_MOBILE=1 NODE_ENV=staging snowpack build --config snowpack.mobile.json",
    "dev:web": "npm run cp-web && snowpack dev --config snowpack.web.json",
    "build:web-production": "npm run tailwind:prod && npm run clean && npm run cp-web && snowpack build --config snowpack.web.json",
    "build:web-staging": "npm run tailwind:prod && npm run clean && npm run cp-web && NODE_ENV=staging snowpack build --config snowpack.web.json",
    "test": "./test.sh",
    "check:formatting": "../node_modules/.bin/prettier --list-different \"**/*.{ts,tsx,js}\"",
    "check:lint": "../node_modules/.bin/eslint \"**/*.{ts,tsx}\"",
    "check:types": "../node_modules/.bin/tsc --noEmit",
    "tailwind:dev": "tailwind build src/tailwind.directives.css -o src/tailwind.css",
    "tailwind:prod": "NODE_ENV=production tailwind build src/tailwind.directives.css -o src/tailwind.css"
  },
  "dependencies": {
    "@capacitor-community/native-audio": "file:../native-audio",
    "@graywolfai/react-tiniest-router": "^0.3.2",
    "@graywolfai/rest-ts-axios": "^0.2.4",
    "@headlessui/react": "^0.2.0",
    "@jsmith21/svg-loaders-react": "^2.2.2",
    "@reach/skip-nav": "^0.11.2",
    "@sentry/browser": "^5.24.2",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "emotion": "^10.0.27",
    "fast-average-color": "^6.0.3",
    "firebase": "^7.23.0",
    "focus-trap-react": "^8.1.0",
    "framer-motion": "^2.7.6",
    "fuse.js": "^6.4.3",
    "idb": "^5.0.6",
    "mitt": "^2.1.0",
    "neverthrow": "^2.7.1",
    "react": "^16.13.1",
    "react-accessible-dropdown-menu-hook": "^2.1.1",
    "react-aria-modal": "^4.0.0",
    "react-dom": "^16.13.1",
    "react-hotkeys-hook": "^2.3.1",
    "react-icons": "^3.11.0",
    "react-infinite-scroll-component": "^5.1.0",
    "react-loading-skeleton": "^2.1.1",
    "react-modal-hook": "^3.0.0",
    "react-simple-snackbar": "^1.1.10",
    "react-tabs": "^3.1.1",
    "react-useportal": "^1.0.13",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "resize-observer-polyfill": "^1.5.1",
    "runtypes": "^5.0.1",
    "throttle-debounce": "^2.3.0",
    "tinycolor2": "^1.4.2",
    "use-resize-observer": "^6.1.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@capacitor/android": "^2.4.2",
    "@capacitor/cli": "^2.4.2",
    "@capacitor/core": "^2.4.2",
    "@capacitor/ios": "^2.4.2",
    "@snowpack/app-scripts-react": "^1.11.0",
    "@snowpack/plugin-webpack": "^2.0.16",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.49",
    "@types/react-aria-modal": "^4.0.2",
    "@types/react-dom": "^16.9.8",
    "@types/react-slider": "^1.0.0",
    "@types/react-tabs": "^2.3.2",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "@types/throttle-debounce": "^2.1.0",
    "@types/tinycolor2": "^1.4.2",
    "@types/uuid": "^8.3.0",
    "dotenv": "^8.2.0",
    "fake-indexeddb": "^3.1.2",
    "patch-package": "^6.2.2",
    "react-test-renderer": "^16.13.1",
    "snowpack": "^2.12.1",
    "tailwindcss": "^1.8.10"
  }
}
